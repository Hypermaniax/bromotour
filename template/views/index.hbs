            {{>feature}}
        </div>
        <!-- Feature-Area End -->

        <!-- Galery Start -->
        <div class="galery py-5">
            {{>galery}}
        </div>
        <!-- Galery End -->

        <!-- Contac Us Start  -->
        <div class="contact-us">
            {{>contact}}
        </div>
        <!-- Contact Us End -->

    </main>

    <!-- Footer Start -->
    <div class="footer-area bg-dark py-5">
        {{>footer}}
    </div>
    <!-- Footer End -->


    <!-- owl carousel -->
